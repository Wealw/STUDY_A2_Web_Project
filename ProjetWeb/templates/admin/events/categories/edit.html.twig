{% extends 'base.html.twig' %}

{% block title 'Editer la catégorie - Administration | BDE-CESI' %}

{% block body %}
    <h1>Editer la catégorie - {{ event_type.eventTypeName }}</h1>
    <div class="form">
        {{ form_start(form) }}
        <div class="group off">{{ form_row(form.event_type_name) }}</div>
        <button type="submit" class="contained">ÉDITER LA CATÉGORIE</button>
        {{ form_end(form) }}
    </div>
{% endblock %}

{% block javascripts %}
    <script src="{{ asset('assets/js/form.js') }}"></script>
{% endblock %}